{
  "name": "agency11",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev --port 4002 --host 0.0.0.0",
    "start": "astro dev",
    "build": "astro build",
    "build:fast": "astro build",
    "preview": "astro preview --host",
    "astro": "astro",
    "perf:mobile": "lhci autorun --collect.url=https://www.edgeviewfinance.com.au --collect.settings.preset=mobile",
    "perf:desktop": "lhci autorun --collect.url=https://www.edgeviewfinance.com.au --collect.settings.preset=desktop",
    "perf:mobile:json": "lighthouse https://www.edgeviewfinance.com.au --form-factor=mobile --output=json --output-path=./performance-reports/mobile.json",
    "perf:desktop:json": "lighthouse https://www.edgeviewfinance.com.au --form-factor=desktop --output=json --output-path=./performance-reports/desktop.json",
    "perf:both": "npm run perf:mobile:json && npm run perf:desktop:json",
    "perf:ci": "lhci autorun",
    "perf:assert": "lhci assert",
    "perf:upload": "lhci upload",
    "monitor:uptime": "node scripts/uptime-server.js",
    "monitor:uptime:once": "node scripts/uptime-server.js --once",
    "monitor:gsc": "node scripts/google-search-console-setup.js",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject ./dist && sentry-cli sourcemaps upload ./dist",
    "mcp:playwright:init": "node scripts/mcp-playwright-init.js init",
    "mcp:playwright:status": "node scripts/mcp-playwright-init.js status",
    "mcp:playwright:shutdown": "node scripts/mcp-playwright-init.js shutdown",
    "mcp:playwright:cleanup": "node scripts/mcp-playwright-init.js cleanup"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.0",
    "@astrojs/mdx": "^4.3.5",
    "@astrojs/react": "^4.3.1",
    "@astrojs/sitemap": "^3.5.1",
    "@astrojs/tailwind": "^6.0.2",
    "@astrojs/vercel": "^8.2.7",
    "@fontsource/cabin": "^5.0.18",
    "@fontsource/instrument-sans": "^5.2.6",
    "@fontsource/inter": "^5.0.18",
    "@fontsource/plus-jakarta-sans": "^5.0.20",
    "@fontsource/public-sans": "^5.0.18",
    "@fortawesome/fontawesome-free": "^6.5.2",
    "@microsoft/clarity": "^1.0.0",
    "@sentry/astro": "^10.11.0",
    "@sentry/node": "^10.11.0",
    "@sentry/react": "^10.22.0",
    "@supabase/supabase-js": "^2.57.0",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@vercel/speed-insights": "^1.2.0",
    "astro": "^5.13.0",
    "dotenv": "^17.2.2",
    "github-slugger": "^2.0.0",
    "jos-animation": "^0.9.2",
    "playwright": "^1.55.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-toastify": "^10.0.5",
    "resend": "^6.0.2",
    "swiper": "^11.1.1",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "@lhci/cli": "^0.15.1",
    "@playwright/mcp": "^0.0.37",
    "@sentry/cli": "^2.53.0",
    "googleapis": "^159.0.0",
    "lighthouse": "^12.8.2",
    "node-fetch": "^3.3.2",
    "tailwindcss": "^3.4.3"
  }
}
