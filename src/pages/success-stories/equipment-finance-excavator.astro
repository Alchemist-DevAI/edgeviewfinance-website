      page_location: window.location.href,
      case_study_type: 'equipment-finance',
      business_type: 'concreting',
      finance_amount: '180000'
    });
  }

  // Track CTA clicks
  const ctaButtons = document.querySelectorAll('a[href*="contact"], a[href*="tel:"]');
  ctaButtons.forEach(button => {
    button.addEventListener('click', function() {
      if (typeof gtag !== 'undefined') {
        gtag('event', 'cta_click', {
          cta_location: 'case_study_page',
          cta_type: this.href.includes('tel:') ? 'phone' : 'contact_form',
          case_study_type: 'equipment-finance'
        });
      }
    });
  });

  // Track related story clicks
  const relatedLinks = document.querySelectorAll('a[href*="/success-stories/"]');
  relatedLinks.forEach(link => {
    link.addEventListener('click', function() {
      if (typeof gtag !== 'undefined') {
        gtag('event', 'related_story_click', {
          from_case_study: 'equipment-finance-excavator',
          to_case_study: this.href.split('/').pop()
        });
      }
    });
  });
});
</script>