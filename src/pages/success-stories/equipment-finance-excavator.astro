  height: 48px;
  background: #28a745;
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
  color: white;
  font-size: 1.5rem;
  margin-bottom: 16px;
}

.nav-button {
  display: inline-flex;
  align-items: center;
  padding: 8px 16px;
  background: white;
  color: #1C2C3B;
  text-decoration: none;
  border-radius: 6px;
  font-size: 0.9rem;
  font-weight: 500;
  transition: all 0.3s ease;
  border: 1px solid #ddd;
}

.nav-button:hover {
  background: #f8f9fa;
  border-color: #FF9E10;
  transform: translateY(-1px);
}

.nav-button-primary {
  background: #FF9E10;
  color: white;
  border-color: #FF9E10;
}

.nav-button-primary:hover {
  background: #e8890e;
  border-color: #e8890e;
}

@media (max-width: 768px) {
  .case-study-hero {
