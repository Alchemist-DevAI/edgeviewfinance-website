<style>
/* Case Study Page Specific Styles */
.case-study-hero {
  background: linear-gradient(135deg, #1C2C3B 0%, #2A3F56 100%);
  padding: 80px 0 60px;
  color: white;
}

.case-study-nav {
  padding: 16px 0;
  background: #f8f9fa;
  border-bottom: 1px solid #e9ecef;
}

.executive-summary {
  background: linear-gradient(135deg, #fff5e6 0%, #fff8f0 100%);
  border-left: 4px solid #FF9E10;
  padding: 32px;
  margin: 40px 0;
  border-radius: 8px;
}

.challenge-section {
  background: #f8f9fa;
  padding: 60px 0;
}

.solution-section {
  background: white;
  padding: 60px 0;
}

.results-section {
  background: linear-gradient(135deg, #e6f7ff 0%, #f0fbff 100%);
  padding: 60px 0;
}

.timeline-section {
  background: white;
  padding: 60px 0;
}

.client-experience-section {
  background: #1C2C3B;
  color: white;
  padding: 60px 0;
}

.final-cta-section {
  background: linear-gradient(135deg, #FF9E10 0%, #e8890e 100%);
  color: white;
  padding: 80px 0;
}

.timeline-item {
  display: flex;
  align-items: flex-start;
  margin-bottom: 32px;
  position: relative;
}

.timeline-item:not(:last-child)::after {
  content: '';
  position: absolute;
  left: 15px;
  top: 40px;
  height: 40px;
  width: 2px;
  background: #FF9E10;
}

.timeline-dot {
  width: 32px;
  height: 32px;
  background: #FF9E10;
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
  color: white;
  font-weight: 600;
  font-size: 14px;
  margin-right: 24px;
  flex-shrink: 0;
}

.timeline-content h4 {
  margin: 0 0 8px 0;
  color: #1C2C3B;
  font-weight: 600;
}

.timeline-content p {
  margin: 0;
  color: #666;
  line-height: 1.6;
}

.stat-card {
  background: white;
  padding: 32px 24px;
  border-radius: 12px;
  text-align: center;
  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
  border-top: 4px solid #FF9E10;
}

.stat-number {
  font-size: 2.5rem;
  font-weight: 700;
  color: #1C2C3B;
  margin-bottom: 8px;
  display: block;
}

.stat-label {
  color: #666;
  font-size: 0.95rem;
  margin-bottom: 4px;
}

.stat-description {
  color: #888;
  font-size: 0.85rem;
  line-height: 1.4;
}

.quote-large {
  font-size: 1.5rem;
  line-height: 1.6;
  color: white;
  font-style: italic;
  margin-bottom: 24px;
  position: relative;
  padding-left: 40px;
}

.quote-large::before {
  content: '"';
  position: absolute;
  left: 0;
  top: -10px;
  font-size: 4rem;
  color: #FF9E10;
  font-family: serif;
}

.client-info {
  display: flex;
  align-items: center;
  margin-top: 32px;
}

.client-avatar {
  width: 60px;
  height: 60px;
  background: #FF9E10;
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
  color: white;
  font-weight: 600;
  font-size: 1.5rem;
  margin-right: 16px;
